			}
		},
		{
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
			"scope": [
				"entity.name.function",
                "support.function",
			],
			"settings": {
				"foreground": "#a2b3ff"
			}
		},
		{
			"scope": [
				"meta.function-call",
				"variable.function",
				"keyword.other.special-method",
			],
			"settings": {
				"foreground": "#89ffd8"
			}
		},
        {
            "name": "Meta.Macro",
            "scope":[
                // "meta.preprocessor.macro",
                "meta.macro",
                "keyword.other.typedef"
            ],
            "settings": {
                "foreground": "#ff91e3",
            }
        },
        {//rust
            "name": "Rust other",
            "scope":[
                "storage.type.rust"
            ],
            "settings": {
