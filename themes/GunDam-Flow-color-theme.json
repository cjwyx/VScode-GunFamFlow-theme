    "tokenColors":[
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#425a55"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "source",
                "variable",
                "string constant.other.placeholder",
                "other"
            ],
            "settings": {
                "foreground": "#ffe7e7ef"
            }
        },
		{
            "name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff538c"
			}
		},
		{
            "name": "Invalid.deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#d192ea"
			}
		},
        {
            "name": "Constant Number char",
            "scope":[
                "constant.numeric",
                "constant.character",
                "constant.language.bool",
                "constant.language.boolean"
