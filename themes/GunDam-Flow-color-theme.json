    "tokenColors":[
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#38665c"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder",
                "other"
            ],
            "settings": {
                "foreground": "#ffd5d5"
            }
        },
		{
            "name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff538c"
			}
		},
		{
            "name": "Invalid.deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#d192ea"
			}
		},
        {
            "name": "Constant Number char",
            "scope":[
                "constant.numeric",
                "constant.character",
                "constant.language.bool",
                "constant.language.boolean"
            ],
            "settings":{
                "foreground": "#ff4b4b",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Constant.language",
            "scope":[
                "constant.language",
            ],
            "settings":{
                "foreground": "#ff769f",
                "fontStyle": "bold"
            }
            
        },
		{
			"scope": [
				"source.cpp meta.block variable.other"
			],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
            "name": "String",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				// "entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D",
			}
		},
        {
            "name": "Storage.modifier",
            "scope":[
                "storage.modifier"
            ],  
            "settings": {
                "foreground": "#ff9100",
                // "fontStyle": "boldW"
            }
        },
        {
            "name": "Storage.Type",
            "scope":[
                "storage.type.primitive",
                "storage.type.built-in",
                "storage.type.haskell",
                // "entity.name.type.parameter",
                // "entity.name.type.class",
                "entity.name.type.primitive",
                // "entity.name.type.result",
                "storage.type.numeric",
                "storage.type.string",
                "entity.name.type.numeric.rust",
                // "support.type.python"
                
            ],
            "settings": {
                "foreground": "#ffa600",
            }
        },
        {
            "name": "Entity.name.type.class",
            "scope":[
                "entity.name.type",
                "entity.name.type.class",
                "entity.name.type.rust",
                // "meta.class"
            ],
            "settings": {
                "foreground": "#ffc400",
            }
        },
        {
            "name":"Entity.name.namespace",
            "scope":[
                "entity.name.namespace"
            ],
            "settings": {
                "foreground": "#e5ff00",
            }
        },
        {
            "name":"Storage.type",
            "scope":[
                "storage.type"
            ],
            "settings": {
                "foreground": "#ffe89d",
            }
        },
        {
            "name": "Storage.type.function",
            "scope":[
                "storage.type.function",
                "keyword.other.fn.rust",
                "keyword.function.go"
                // "meta.function.definition"
            ],
            "settings": {
                "foreground": "#56adff",
                "fontStyle":"italic bold"
            }
        },
        {
            "name": "Storage.type.class",
            "scope":[
                "storage.type.class",
                // "meta.function.definition"
            ],
            "settings": {
                "foreground": "#9d92ff",
                "fontStyle":"italic bold"
            }
            
        },
        {
            "name": "Keyword.Control",
            "scope":[
                "keyword.control",
                "keyword.other.import",
                "keyword.other.use",

                // "keyword.other.rust"
                "keyword.other.import",
                "keyword.import.go"
                
                
            ],
            "settings": {
                "foreground": "#c599ff",
                "fontStyle":"italic bold"
            }
        },
