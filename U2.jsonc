        "meta.qualified_type",

        "keyword.other.rust"
    ],
    "settings": {
        "foreground": "#ffde4a",
    }
},
{
    "name": "Storage Modifier",
    "scope": [
        "storage.modifier"
    ],
    "settings": {
        "foreground": "#ff9100"
