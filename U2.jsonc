{
    "name": "Comment",
    "scope": [
        "comment",
        "punctuation.definition.comment"
    ],
    "settings": {
        "fontStyle": "italic",
        "foreground": "#38665c"
    }
},
{
    "name": "Invalid",
    "scope": [
        "invalid",
        "invalid.illegal"
    ],
    "settings": {
        "foreground": "#FF5370"
    }
},
{
    "name": "Variables",
    "scope": [
        "variable",
        "string constant.other.placeholder",
        "other"
    ],
    "settings": {
        "foreground": "#ffe2d2"
    }
},
{
    "name": "Storage.Type",
    "scope":[
        "storage.type.primitive",
        "storage.type.built-in",
